OBJS_REAL = $(foreach obj, $(OBJS), $(OBJS_OUTPUT_DIR)/$(obj))

.PHONY: all
all: setup $(LIB_NAME)

setup:
	[ -d $(OBJS_OUTPUT_DIR) ] || mkdir $(OBJS_OUTPUT_DIR)

$(LIB_NAME): $(OBJS)
	$(AR) -r $@ $(OBJS_REAL)

%.o: %.c
	$(CC) $(CFLAGS) -c -o $(OBJS_OUTPUT_DIR)/$@ $<

######################################################################

clean:
	rm -rvf $(OBJS_OUTPUT_DIR)
	rm -f $(LIB_NAME)
